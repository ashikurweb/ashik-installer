{
    "name": "ashik/laravel-installer",
    "description": "A simple installer for Laravel applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashikur Rahman",
            "email": "ashikurrahman7194@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Ashik\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashik\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}